<!DOCTYPE html>
<html lang="pt-br" ng-app="appBase">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=9" />
    <title>{% block title %}Ibram Core Base{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <link type="text/css" rel="stylesheet" href="{{ asset('bundles/ibramcorebase/css/bootstrap.min.css') }}" />
    <link type="text/css" rel="stylesheet" href="{{ asset('bundles/ibramcorebase/css/bootstrap-responsive.min.css') }}" />
    <link type="text/css" rel="stylesheet" href="{{ asset('bundles/ibramcorebase/css/datepicker.css') }}" />
    <link type="text/css" rel="stylesheet" href="{{ asset('bundles/ibramcorebase/css/menu.css') }}" />
    <link type="text/css" rel="stylesheet" href="{{ asset('bundles/ibramcorebase/css/contraste.css') }}" />
    <link type="text/css" rel="stylesheet" href="{{ asset('bundles/ibramcorebase/css/template.css') }}" />
    <link type="text/css" rel="stylesheet" href="{{ asset('bundles/ibramcorebase/css/content.css') }}" />
    <link type="text/css" rel="stylesheet" href="{{ asset('bundles/ibramcorebase/css/ui-lightness/jquery-ui-1.10.3.custom.min.css') }}" />
    <link type="text/css" rel="stylesheet" href="{{ asset('bundles/ibramcorebase/css/ui.jqgrid.css') }}" />
    
    {% block css %}
    {% endblock %}
    
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="{{ asset('bundles/ibramcorebase/js/html5shiv.js') }}"></script>
    <![endif]-->
    <!-- Fav and touch icons -->
    <link rel="shortcut icon" href="{{ asset('favicon.ico') }}">

      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/jquery-1.10.2.min.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/validation/jquery.validate.min.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/validation/localization/messages_pt_BR.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/bootstrap.min.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/bootstrap-tab.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/bootstrap-datepicker.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/contraste.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/store.min.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/rv-jquery-fontsize-2.0.3.min.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/functions.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/jquery-ui-1.10.3.custom.min.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/i18n/grid.locale-pt-br.js') }}" type="text/javascript"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/jquery.jqGrid.min.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/jquery.maskedinput.min.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/ui.datepicker-pt-BR.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/set-environment.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/maskedMoney.3.0.2.js') }}"></script>
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/inputMasks.js') }}"></script>
      {% block session_counter_js %}
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/timerSession.js') }}"></script>
      {% endblock %}
      <script type="text/javascript" src="{{ asset('bundles/ibramcorebase/js/jquery.monthpicker.js') }}"></script>
    
    <script type="text/javascript">
      $(document).ready(function() { 
          
          $.rvFontsize({
              targetSection: '#content',
              store: true, // store.min.js required!
              controllers: {
                  appendTo: '#rvfs-controllers',
                  showResetButton: true
              }
          });

      });

    </script>
    {% block javascript_libs %}
    {% endblock %}
  </head>
  <body id="content">
    <div id="loading" style="display: none;">
        <div class="overlay" style=" background-color: #000; width: 100%; height: 100%; float: left; position: absolute; left: 0; top: 0; z-index: 99; opacity: 0.4;"></div>
        <img style="position: absolute;top: 35%;z-index: 99;left: 45%;width: 205px;height: 305px;"
             src="{{ asset('bundles/Ibramsgpmaismedicos/images/ajax-loader-transparente.gif') }}">
    </div>
    <div id="dialog-msg" title="Mensagem do Sistema" style="display: none;"></div>
    <div id="session-dialog" title="Mensagem do Sistema" style="display: none;"></div>
    <div class="header">
      <div id="barra-brasil">
        <div class="container">
          <div class="row">
            <div class="span12">
              <div class="barra">
                <ul>
                    <li><a title="Acesso à informação" class="ai" href="http://www.acessoainformacao.gov.br" target="_parent">www.sic.gov.br</a></li>
                    <li><a title="Portal de Estado do Brasil" class="brasilgov" href="http://www.brasil.gov.br" target="_parent">www.brasil.gov.br</a></li>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div id="topo_geral">
        <div class="container">
          <div class="row">
            <div class="span8">
              <div class="logo">{% block sigla_sistema %}BASE{% endblock %}</div>
              <div class="titulo_sistema">
                <span class="img_separador"></span>
                <h1>{% block nome_sistema %}IBRAM{% endblock %}</h1>
                <h3>{% block descricao_sistema %}Core Base de Desenvolvimento de Sistemas PHP do Ibram{% endblock %}</h3>
              </div>
            </div>
            <div class="span4">
              <div class="box_botoes_acessibilidade">
                <div id="rvfs-controllers" class="fontsize-controllers group"></div>
                <a title="Contraste" href="#" id="lbkContraste" onclick="atribuirContraste()" class="contraste">
                  <img src="{{ asset('bundles/ibramcorebase/images/lgo/bt_acess_contraste.png') }}" width="13" height="13"/>
                </a>
              </div>
              <div id="box_menu_top" >
                <ul>
                    {% block box_menu_top %} {% endblock %}
                    {% block menu_sair %}
                    <li class="sair">
                        <a href="{{ path('ibram_core_base_logout') }}"><span class="icon-off icon-white"></span>Sair</a>
                    </li>
                    {% endblock %}
                </ul>
              </div>
            </div>
            <div class="usuario">
              <p>{{ render(controller('IbramCoreBaseBundle:Default:sessionInfo')) }}</p>
            </div>
          </div>
        </div>
      </div>
    <div class="BoxMenuPrincipalTamanho">
        <div id="BoxMenuPrincipal">
        <div class="container">
          <div class="row">
            <div class="span8">
              <div class="navbar nav">
                <a data-target=".navbar-responsive-collapse" data-toggle="collapse" class="btn btn-navbar select">
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                </a>
                <div class="nav-collapse navbar-responsive-collapse collapse">
                {% block page_menu %}{% if app.session.get('user_name') %}{{ knp_menu_render('IbramCoreBaseBundle:Builder:menuExemplo', {'nav_type': 'navbar', 'template':'IbramCoreBaseBundle:menu:ibram_menu.html.twig'}) }}{% endif %}{% endblock %}
                </div>
              </div>
            </div>
            <div class="span4">
              <div id="box_info_usuario">
              {% block extra_info %}
                <div class="menuInformacao pull-left">
                      <a class="icoInformacao dropdown-toggle margin-right perfil" data-placement="top" rel="tooltip" data-toggle="dropdown" data-original-title="Informações" href="#"></a>
                      <ul class="dropdown-menu subMenuInforma" style="width: 214px;">
                          <!-- <li class="nav-header">PROVAB</li> -->
                          <li>
                            <span class="imgPrograma" style="float:left;">
                                <img src="{{ asset('bundles/Ibramsgpcore/images/lgo/maisMedicosInformacao.png') }}" alt="Programa">
                            </span>
                              <ul class="noMargin subIntem">
                                  <li><a href="http://www.saude.gov.br/maismedicos">Portal</a></li>
                                  <li><a href="/manuais.php">Documentos</a></li>
                                  <li><a href="/faq.php">FAQ</a></li>
                              </ul>
                          </li>
                          <li>
                            <span class="imgPrograma" style="float:left;">
                                <img src="{{ asset('bundles/Ibramsgpcore/images/lgo/provabeInfInformacao.png') }}" alt="Programa">
                            </span>
                              <ul class="noMargin subIntem">
                                  <li><a href="http://www.saude.gov.br/maismedicos">Portal</a></li>
                                  <li><a href="/manuaisProvabEnfDent.php">Documentos</a></li>
                                  <li><a href="/faqProvabEnfDent.php">FAQ</a></li>
                              </ul>
                          </li>
                          <li class="ultima">
                            <span class="imgPrograma" style="float:left;">
                                <img src="{{ asset('bundles/Ibramsgpcore/images/lgo/provabeInformacao.png') }}" alt="Programa">
                            </span>
                              <ul class="noMargin subIntem">
                                  <li><a href="http://portal.saude.gov.br/portal/saude/profissional/visualizar_texto.cfm?idtxt=39675&janela=1">Portal</a></li>
                                  <li><a href="http://189.28.128.100/maismedicos/Passo-a-passo-da-Validacao-Bolsas-PROVAB-2013.pdf">Documentos</a></li>
                                  <li><a href="http://189.28.128.100/maismedicos/FAQ EDITAL PROVAB.pdf">FAQ</a></li>
                              </ul>
                          </li>
                      </ul>
                  </div>
              {% endblock %}
              {% block session_counter %}
                <div id="box_info_usuario_tempo">
                    <div id="wrapper-clock-count">Sua sessão expira em: <span id="keepAlive"></span></div>
                </div>
              {% endblock %}
              </div>
            </div>
          </div>
        </div>
      </div>
      </div>
    <div class="section" style="min-height:498px">
      <div class="container">
          <div class="">
            <div class="box">
                {% for label, flashes in app.session.flashbag.all %}
					{% for flash in flashes %}
						<div class="alert alert-{{ label }}">
						  {{ flash | replace({"\n":'<br />'}) | raw }}

						</div>
						
					{% endfor %}
				{% endfor %}
              {% block body %}{% endblock %}
          </div>
        </div>
      </div>
    </div>
    {% block footer %}
    <footer>
      <div id="BoxRodapePrincipal">
        <div class="container">
          <div class="row">
            <div class="span6">
              <div id="BoxRodape_endereco">
              {% block endereco_footer %}
                <p>
                &copy; Instituto Brasileiro de Museus – IBRAM<br />
				SBN, quadra 2, lote 8, bloco N, Edifício CNC III<br />
				Brasília - DF - CEP 70040-020
                </p>
              {% endblock %}
              </div>
            </div>
            <div class="span6">
              <div id="BoxRodape_LogoIbram">Ibram</div>
            </div>
          </div>
        </div>
      </div>
    </footer>
    {% endblock %}
    	<script>
            window.jQuery || document.write('<script src="{{ asset('bundles/ibramcorebase/js/jquery.js') }}"><\/script>')
            $('#dialog-msg').dialog({title: "Inicializando",
                resizable: false,
                height: 200,
                width: 300,
                modal: true,
                close: false}).html('Aguarde...');
            $('#dialog-msg').dialog('close');
            window.jQuery || document.write('<script src="{{ asset('bundles/ibramcorebase/js/jquery.js') }}"><\/script>')
        </script>
    	{% block javascripts %}{% endblock %}
  </body>
</html>